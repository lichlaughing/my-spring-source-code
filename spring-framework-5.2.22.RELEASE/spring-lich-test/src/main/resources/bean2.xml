<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "https://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="user" class="cn.lichenghao.entity.User">
		<property name="name">
			<value>Rod</value>
		</property>
		<property name="age">
			<value>31</value>
		</property>
	</bean>
	<bean id="myBeanFactoryPostProcessor" class="cn.lichenghao.context.MyBeanFactoryPostProcessor"/>
	<bean id="myBeanFactoryPostProcessor2" class="cn.lichenghao.context.MyBeanFactoryPostProcessor2"/>
</beans>